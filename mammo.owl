    <!-- http://www.semanticweb.org/tuixiang/ontologies/mammo#不定形钙化 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#不定形钙化">
        <rdf:type rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#描述"/>
        <describe_by rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#钙化形态"/>
        <具体描述>不定形钙化:形态小而模糊,常无典型特征,弥漫分布常为良性表现,而成簇、区域、线样和叶段分布者需活检定性。</具体描述>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/tuixiang/ontologies/mammo#不规则肿块 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#不规则肿块">
