    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#钙化区域分布">
        <rdf:type rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#描述"/>
