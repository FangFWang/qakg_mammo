    <!-- http://www.semanticweb.org/tuixiang/ontologies/mammo#圆形肿块 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#圆形肿块">
        <rdf:type rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#描述"/>
        <describe_by rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#肿块形态"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/tuixiang/ontologies/mammo#圆形钙化 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#圆形钙化">
        <rdf:type rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#描述"/>
        <describe_by rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#钙化形态"/>
        <具体描述>(5)圆形钙化:多发者大小不一,通常直径&lt;1.0 mm,常位于腺泡中。直径I&gt;0.5 mm称为圆点状钙化,直径&lt;0.5 mm称为针尖状钙化</具体描述>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/tuixiang/ontologies/mammo#小分叶边缘 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#小分叶边缘">
        <rdf:type rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#描述"/>
        <describe_by rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#肿块边缘"/>
        <具体描述>小分叶指病灶边缘呈小波浪状改变,小分叶边缘为常见的恶性征象。</具体描述>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/tuixiang/ontologies/mammo#椭圆形肿块 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#椭圆形肿块">
        <rdf:type rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#描述"/>
        <describe_by rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#肿块形态"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/tuixiang/ontologies/mammo#毛刺边缘 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#毛刺边缘">
        <rdf:type rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#描述"/>
        <describe_by rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#肿块边缘"/>
        <具体描述>毛刺边缘是病灶边缘向外发出的放射状线影,长短不一,根部较粗。毛刺边缘为常见的恶性征象。</具体描述>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/tuixiang/ontologies/mammo#浸润边缘 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#浸润边缘">
        <rdf:type rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#描述"/>
        <describe_by rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#肿块边缘"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/tuixiang/ontologies/mammo#环形钙化 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#环形钙化">
        <rdf:type rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#描述"/>
        <describe_by rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#钙化形态"/>
        <具体描述>环形钙化:壁厚度超过蛋壳样钙化,直径为1—10 mm,甚至更大。边缘光整呈圆形或卵圆形,中央为低密度或透亮。常见于脂肪坏死、积油囊肿和导管钙化。</具体描述>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/tuixiang/ontologies/mammo#皮肤钙化 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#皮肤钙化">
        <rdf:type rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#描述"/>
        <describe_by rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#钙化形态"/>
        <具体描述>皮肤钙化:典型者呈小环状,中心呈透亮改变;可多个小环成簇分布。不典型者可借助切线投照予以鉴别。</具体描述>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/tuixiang/ontologies/mammo#等密度肿块 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#等密度肿块">
        <rdf:type rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#描述"/>
        <describe_by rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#肿块密度"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/tuixiang/ontologies/mammo#粗棒状钙化 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#粗棒状钙化">
        <rdf:type rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#描述"/>
        <describe_by rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#钙化形态"/>
        <具体描述>粗棒状钙化:直径通常&gt;1 mm,发生于导管腔内,其形态顺应导管腔,又称铸型钙化。可多节段沿导管走行方向排列,边缘清楚,偶可呈分支状或中央透亮改变。常见于分泌性病变(浆细胞性乳腺炎)。</具体描述>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/tuixiang/ontologies/mammo#粗糙不均质钙化 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#粗糙不均质钙化">
        <rdf:type rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#描述"/>
        <describe_by rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#钙化形态"/>
        <具体描述>粗糙不均质钙化:钙化形态不规则,直径多&gt;0.5 mm,有聚集趋势者可能为恶性改变,也可出现在纤维化、纤维腺瘤和外伤后的乳腺中,需要结合其分布情况判断良、恶性。</具体描述>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/tuixiang/ontologies/mammo#粗糙或爆米花样钙化 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#粗糙或爆米花样钙化">
        <rdf:type rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#描述"/>
        <describe_by rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#钙化形态"/>
        <具体描述>粗糙或爆米花样钙化:呈粗颗粒样或斑块样,钙化直径&gt;2~3 mm,为纤维腺瘤钙化的特征表现。</具体描述>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/tuixiang/ontologies/mammo#线性或线样分支状钙化 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#线性或线样分支状钙化">
        <rdf:type rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#描述"/>
        <describe_by rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#钙化形态"/>
        <具体描述>线样或线样分支状钙化(恶性铸型钙化):呈纤细的短线(直线或曲线)断续沿导管走行方向排列的不规则境界模糊的钙化,粗细小于&lt;0.5 mm。其中部分钙化可境界清楚,提示导管癌导管内成分的充填。可呈线样、成簇、叶段及区域性分布,主要表现为多发的不规则短曲线状钙化称为蠕虫状钙化。高度恶性可能的钙化</具体描述>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/tuixiang/ontologies/mammo#细小多形性钙化 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#细小多形性钙化">
        <rdf:type rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#描述"/>
        <describe_by rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#钙化形态"/>
        <具体描述>细小多形性钙化:形态、大小不一,直径常&lt;0.5 mm。成簇或叶段分布。高度恶性可能的钙化</具体描述>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/tuixiang/ontologies/mammo#缝线钙化 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#缝线钙化">
        <rdf:type rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#描述"/>
        <describe_by rdf:resource="http://www.semanticweb.org/tuixiang/ontologies/mammo#钙化形态"/>
        <具体描述>缝线钙化:钙质沉积在缝线材料上所致。典型者为弧线形、绳结样改变。</具体描述>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/tuixiang/ontologies/mammo#肿块 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuixiang/ontologies/mammo#肿块">
